<script>
(function () {
    "use strict";
$(window).load(function () {
        var grid = document.querySelector('#masonry');
        var msnry;
        imagesLoaded(grid, function () {
            if (Modernizr.csstransitions) {
                msnry = new Masonry(grid, {
                    itemSelector: '.item',
                    columnWidth: 1,
                    percentPosition: true,
                    transitionDuration: 0
                });
            } else {
                msnry = new Masonry(grid, {
                    itemSelector: '.item',
                    columnWidth: 1,
                    percentPosition: true,
                    transitionDuration: '.2s'
                });
            }
        });
    });
}());
</script>
 <script>
        var window_top = $(window);
        function navbar_stick() {
            if (window_top.scrollTop() >  176) {
                $('#navbar').addClass('scroll-stick');
                $('#place-holder').removeClass('remove');
            } else {
                $('#navbar').removeClass('scroll-stick');
                $('#place-holder').addClass('remove');
            }
        }
        $(window).load(function () {
           $(window).scroll(navbar_stick);
           navbar_stick();
           $('.item').addClass('container-load');
        });
</script>
